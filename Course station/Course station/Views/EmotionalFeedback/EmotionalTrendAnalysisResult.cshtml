@model IEnumerable<Course_station.Controllers.EmotionalFeedbackController.EmotionalTrendAnalysisResult>

@{
    ViewData["Title"] = "Emotional Trend Analysis Result";
}

<h1>Emotional Trend Analysis Result</h1>

<table class="table">
    <thead>
        <tr>
            <th>Activity ID</th>
            <th>Emotional State</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.ActivityId</td>
                <td>@item.EmotionalState</td>
            </tr>
        }
    </tbody>
</table>
